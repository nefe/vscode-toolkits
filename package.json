{
  "name": "nelfe-toolkits",
  "displayName": "nelfe-toolkits",
  "description": "",
  "version": "0.0.3",
  "publisher": "jasonHzq",
  "engines": {
    "vscode": "^1.19.0"
  },
  "categories": ["Other", "Snippets"],
  "activationEvents": [
    "onCommand:nelfe.addAction",
    "onCommand:nelfe.findI18N",
    "onLanguage:typescript"
  ],
  "main": "./out/extension",
  "capabilities": {
    "codeLensProvider": {
      "resolveProvider": "true"
    }
  },
  "contributes": {
    "commands": [
      {
        "command": "nelfe.addAction",
        "title": "add action"
      },
      {
        "command": "nelfe.findI18N",
        "title": "find i18n cn"
      }
    ],
    "keybindings": [
      {
        "command": "nelfe.addAction",
        "key": "cmd+ctrl+a",
        "mac": "cmd+ctrl+a"
      },
      {
        "command": "nelfe.findI18N",
        "key": "cmd+ctrl+f",
        "mac": "cmd+ctrl+f"
      }
    ],
    "snippets": [
      {
        "language": "typescript",
        "path": "./snippets/redux.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/redux.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6"
  },
  "dependencies": {
    "ts-node": "^5.0.1",
    "typescript": "^2.6.2"
  }
}
