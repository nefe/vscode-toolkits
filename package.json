{
  "name": "nelfe",
  "displayName": "nelfe",
  "description": "",
  "version": "0.0.2",
  "publisher": "jasonHzq",
  "engines": {
    "vscode": "^1.19.0"
  },
  "categories": ["Other", "Snippets"],
  "activationEvents": ["onCommand:nelfe.addAction"],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "nelfe.addAction",
        "title": "add action"
      }
    ],
    "keybindings": [
      {
        "command": "nelfe.addAction",
        "key": "cmd+ctrl+a",
        "mac": "cmd+ctrl+a"
      }
    ],
    "snippets": [
      {
        "language": "typescript",
        "path": "./snippets/redux.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/redux.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.6",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  }
}
